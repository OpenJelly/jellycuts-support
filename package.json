{
	"name": "jellycuts-support",
	"displayName": "Jellycuts Support",
	"description": "Support for the Jellycuts iOS app.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:jellycuts-support.helloWorld",
		"onLanguage:Jelly"
	],
	"main": "./extension.js",
	"contributes": {
		"keybindings": [
			{
				"command": "jellycuts-support.runOnDevice",
				"key": "ctrl+r",
				"mac": "cmd+r"
			},
			{
				"command": "jellycuts-support.exportOnDevice",
				"key": "ctrl+shift+e",
				"mac": "cmd+shift+e"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.52.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"eslint": "^7.6.0",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"internal-ip": "^6.2.0",
		"ip": "^1.1.5",
		"ws": "^7.4.2"
	}
}
